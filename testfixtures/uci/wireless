config wifi-device 'radio1'
        option type 'mac80211'
        option path 'scb/fd500000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
        option band '6g'
        option channel '1'
        option htmode 'HE80'

config wifi-iface 'default_radio1'
        option device 'radio1'
        option network 'ahwlan'
        option mode 'ap'
        option ssid 'TestSSID'
        option key 'thisisnotarealpassword'
        option disabled '1'

config wifi-device 'radio2'
        option type 'mac80211'
        option path 'platform/soc/fe980000.usb/usb1/1-1/1-1.2/1-1.2:1.0'
        option band '2g'
        option channel '7'
        option htmode 'HT20'
        option country 'US'
        option cell_density '0'

config wifi-iface 'default_radio2'
        option device 'radio2'
        option network 'batmesh1'
        option mode 'mesh'
        option key 'thisisnotarealpassword'
        option mesh_id 'openmanet'
        option mesh_fwding '0'
        option mesh_rssi_threshold '0'
        option encryption 'sae'

config wifi-device 'radio3'
        option type 'morse'
        option path 'platform/soc/fe204000.spi/spi_master/spi0/spi0.0'
        option band 's1g'
        option hwmode '11ah'
        option reconf '0'
        option bcf 'bcf_fgh100mhaamd.bin'
        option enable_mcast_whitelist '0'
        option enable_mcast_rate_control '1'
        option country 'US'
        option channel '42'

config wifi-iface 'default_radio3'
        option mode 'mesh'
        option wds '1'
        option device 'radio3'
        option network 'batmesh0'
        option ssid 'TestSSID'
        option encryption 'sae'
        option key 'thisisnotarealpassword'
        option beacon_int '1000'
        option mesh_id 'openmanet'
